// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "ctrl+shift+alt+[BracketLeft]",
        "command": "workbench.action.terminal.new"
    },
    {
        "key": "ctrl+g",
        "command": "workbench.action.nextEditorInGroup"
    },
    {
        "key": "alt+k",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+i",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+oem_comma",
        "command": "editor.unfold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+shift+oem_6",
        "command": "-editor.unfold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+oem_period ctrl+oem_period",
        "command": "editor.unfoldRecursively",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+k ctrl+oem_6",
        "command": "-editor.unfoldRecursively",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+oem_period ctrl+oem_2",
        "command": "editor.fold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+shift+oem_4",
        "command": "-editor.fold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+oem_period ctrl+oem_comma",
        "command": "editor.foldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+k ctrl+0",
        "command": "-editor.foldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+u",
        "command": "workbench.action.focusNextGroup"
    },
    {
        "key": "ctrl+shift+alt+enter",
        "command": "workbench.action.terminal.focusNext"
    },
    {
        "key": "ctrl+v",
        "command": "workbench.action.terminal.paste",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-workbench.action.terminal.paste",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+[BracketLeft]",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+shift+[BracketLeft]",
        "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+c",
        "command": "workbench.action.terminal.copySelection",
        "when": "terminalFocus && terminalTextSelected"
    },
    {
        "key": "ctrl+shift+c",
        "command": "-workbench.action.terminal.copySelection",
        "when": "terminalFocus && terminalTextSelected"
    },
    {
        "key": "ctrl+[Semicolon]",
        "command": "cursorLineEnd"
    },
    {
        "key": "ctrl+shift+[Semicolon]",
        "command": "cursorLineEndSelect"
    },
    {
        "key": "ctrl+m",
        "command": "cursorLineStart"
    },
    {
        "key": "ctrl+shift+m",
        "command": "cursorLineStartSelect"
    },
    {
        "key": "ctrl+k",
        "command": "multiCommand.Down"
    },
    {
        "key": "ctrl+shift+k",
        "command": "multiCommand.DownSelect"
    },
    {
        "key": "ctrl+j",
        "command": "multiCommand.Left"
    },
    {
        "key": "ctrl+shift+j",
        "command": "multiCommand.LeftSelect"
    },
    {
        "key": "ctrl+l",
        "command": "multiCommand.Right"
    },
    {
        "key": "ctrl+shift+l",
        "command": "multiCommand.RightSelect"
    },
    {
        "key": "ctrl+i",
        "command": "multiCommand.Up"
    },
    {
        "key": "ctrl+shift+i",
        "command": "multiCommand.UpSelect"
    },
    {
        "key": "ctrl+shift+o",
        "command": "-workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+shift+u",
        "command": "multiCommand.deleteLeft"
    },
    {
        "key": "ctrl+shift+o",
        "command": "multiCommand.deleteRight"
    }
]